{
    "framework": "create-react-app",
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/bfs.ico",
        "dest": "/bfs.ico"
      },
      {
        "src": "^/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "^/.*",
        "dest": "/index.html"
      }
    ]
  }